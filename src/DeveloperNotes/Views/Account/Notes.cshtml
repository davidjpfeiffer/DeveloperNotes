@model IEnumerable<DeveloperNotes.Models.Note>

@{
    ViewData["Title"] = "My Notes";
}

<h2>My Notes</h2>

<p>
    <a asp-controller="Notes" asp-action="Create" class="btn btn-default">Create New</a>
</p>

<ul>
    @foreach (var item in Model)
    {
        <li><a asp-controller="Notes" asp-action="View" asp-route-id="@item.NoteId">@Html.DisplayFor(modelItem => item.Title)</a></li>
    }
</ul>
